.gl-wrapper {
    width: 970px;
    margin: auto;
    background: url(../img/bg.jpg) repeat 0 0;
    font-family: Arial, sans-serif;
    position: relative;
    color: #333333;
    font-size: 13px;
    line-height: 18px;
}

.gl-top {
    height: 100px;
    background: url(../img/l-top.png) repeat-x 0 0;
    padding: 0 20px;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    z-index: 20;
}

.gl-logo {
    background: url(../img/logo.png) no-repeat 0 0;
    width: 136px;
    height: 122px;
    display: block;
}

.gl-social {
    width: 132px;
    height: 31px;
    background: url(../img/icons.png) no-repeat 0 0;
    float: right;
    position: relative;
    top: 5px;

}

.gl-social__l {
    width: 28px;
    display: inline-block;
    height: 31px;
    margin-left: 2px;
}

.gl-social__l:first-child {
    margin-left: 1px;
}

.gl-homelink {
    font-size: 14px;
    font-style: italic;
    color: #0066cc;
    border-bottom: 1px solid #0066cc;
    text-decoration: none;
    float: right;
    padding-top: 5px;
    margin-right: 35px;

}

.gl-homelink:hover {
    color: #0066cc;
    border-color: transparent;
}

.gl-nav__i {
    font-size: 13px;
    font-weight: bold;
    color: #fff;
    height: 45px;
    display: inline-block;
    padding: 0 12px;
    border-radius: 5px;
    position: relative;
    margin-left: 25px;
    vertical-align: middle;
    text-align: center;
    cursor: pointer;
    line-height: 18px;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    padding-top: 3px;

}

.gl-nav__i:after {
    position: absolute;
    content: '';
    height: 40px;
    width: 1px;
    border-radius: 20px;
    background: #f39511; /* Old browsers */
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top, #f39511 0%, #f3a20b 22%, #fcae02 51%, #f3a20b 81%, #ed9119 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #f39511), color-stop(22%, #f3a20b), color-stop(51%, #fcae02), color-stop(81%, #f3a20b), color-stop(100%, #ed9119)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, #f39511 0%, #f3a20b 22%, #fcae02 51%, #f3a20b 81%, #ed9119 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, #f39511 0%, #f3a20b 22%, #fcae02 51%, #f3a20b 81%, #ed9119 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, #f39511 0%, #f3a20b 22%, #fcae02 51%, #f3a20b 81%, #ed9119 100%); /* IE10+ */
    background: linear-gradient(to bottom, #f39511 0%, #f3a20b 22%, #fcae02 51%, #f3a20b 81%, #ed9119 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f39511', endColorstr='#ed9119', GradientType=0); /* IE6-8 */

    left: -13px;
    top: 7px;
}

.gl-nav {
    clear: both;
    float: right;
    margin-top: 20px;
}

.gl-nav__i:first-child::after {
    width: 0;
}

.gl-nav__i:first-child {
    line-height: 40px;
}

.gl-nav__i:hover {
    background: #f5f5f5;
    color: #f68b1f;
}

.gl-nav__i._active {
    background: #f5f5f5;
    color: #f68b1f;
}

.gl_about {
    background: url(../img/1.jpg) no-repeat 0 0;
    padding-left: 20px;
    padding-bottom: 60px;
    padding-top: 290px;
}

.gl_about__box {
    display: inline-block;
    height: 290px;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    border: 1px solid #d7d7d5;
    vertical-align: top;
}

.shadow {
    position: relative;
}

.shadow:after {
    content: '';
    position: absolute;
    bottom: 7px;
    z-index: -1;
    width: 85%;
    height: 50px;
    box-shadow: 0 5px 10px 5px rgba(0, 0, 0, .2);
    right: 5%;
    -webkit-transform: rotate(2deg);
    -moz-transform: rotate(2deg);
    -ms-transform: rotate(2deg);
    -o-transform: rotate(2deg);
    transform: rotate(2deg);
}

